{
    "name": "create-file-folder-quickly",
    "displayName": "Create File and Folder Quickly",
    "description": "Create files and folders quickly with intelligent path suggestions",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/ArunShresthaa/Create-File-and-Folder"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:quickFileFolder.create"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quickFileFolder.create",
                "title": "Create File/Folder Quickly"
            }
        ],
        "keybindings": [
            {
                "command": "quickFileFolder.create",
                "key": "ctrl+n",
                "mac": "cmd+n"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "16.x",
        "@types/vscode": "^1.60.0",
        "typescript": "^4.9.4"
    }
}