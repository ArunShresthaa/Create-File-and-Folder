{
    "name": "create-file-folder-quickly",
    "displayName": "Create File and Folder Quickly",
    "description": "Create files and folders quickly with intelligent path suggestions",
    "version": "1.0.1",
    "publisher": "ArunShrestha",
    "repository": {
        "type": "git",
        "url": "https://github.com/ArunShresthaa/Create-File-and-Folder"
    },
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other",
        "Snippets"
    ],
    "activationEvents": [
        "onCommand:quickFileFolder.create"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "quickFileFolder.create",
                "title": "Create File/Folder Quickly"
            }
        ],
        "keybindings": [
            {
                "command": "quickFileFolder.create",
                "key": "ctrl+n",
                "mac": "cmd+n"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "16.x",
        "@types/vscode": "^1.60.0",
        "typescript": "^4.9.4",
        "vsce": "^2.15.0"
    },
    "keywords": [
        "file",
        "folder",
        "create",
        "quick",
        "path",
        "suggestions"
    ],
    "license": "MIT"
}